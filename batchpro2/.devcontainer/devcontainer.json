{
  "name": "BatchPro2 - Java 17 Development Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack"
      ]
    }
  },
  "postCreateCommand": "git clone https://github.com/linjing1003/linjingSpring.git /opt/batchpro2 && cd /opt/batchpro2 && mvn install",
  "workspaceFolder": "/workspaces/batchpro2",
  "remoteUser": "root"
}
